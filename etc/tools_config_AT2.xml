			<CapSensor name="compt_test.cap(proximity)" enable="false" priority="10" period="180"/>
			<Compass name="compt_test.compass" enable="true" priority="10" period="180"/>
			<Gyroscope name="compt_test.gyro" enable="false" priority="10" period="180"/>
			<LightSensor name="compt_test.light" enable="false" priority="10" period="180"/>
			<LED name="compt_test.led" enable="false" priority="10" period="180"/>
			<Pressure name="compt_test.pressure" enable="true" priority="10" period="180"/>
		</loop>
	</loop>


	<loop name="5_Step4" priority="540" cycle="1">

		<SetChargingMode name="set_charging_mode" enable="false" priority="1" value="enable"/>

		<loop name="5_Interference_Test_4-1" priority="3" cycle="1">			

				
			<Vibrator name="interference_test.vibrator" enable="false" priority="10" period="1200"/>
			<CPUMath name="interference_test.cpumath" enable="true" priority="10" thread="2" period="1200"/>
			<Wifi name="interference_test.wifi" enable="true" priority="10" period="1200"/>
			<Bluetooth name="interference_test.bt" enable="true" priority="10" period="1200"/>
			<Ethernet name="interference_test.ethernet" enable="false" priority="10" period="1200"/>
			<Backlight name="backlight" enable="false" priority="10" period="1200">
				<Value>255</Value>
				<BacklightPath>/sys/class/leds/lcd-backlight/brightness</BacklightPath>
        		</Backlight>
			<ChangeBackground name="change_background_max" enable="false" priority="10" interval="1" period="1200"/>
			<LCD name="compt_test.lcd" enable="false" priority="10" maxThreshold="1" period="1200"/>
			<RFMaxPower name="interference_test.rfmaxpower" enable="false" priority="10" period="1200">
				<QXDMWorkAround enable="true"/>
			</RFMaxPower>
			<Video name="video" enable="false" volumeMode="max" priority="10" period="1200">
 	 			<VideoPath>/system/etc/QtestVideo.mp4</VideoPath>
			</Video>
		
		</loop>
	

	</loop>


	<loop name="5_Step5" priority="550" cycle="1">

		<SetChargingMode name="set_charging_mode" enable="false" priority="1" value="enable"/>

		<loop name="5_Sleep_Current_Test_5-1" priority="10" cycle="1">
		
			<SuspendResume name="sleep_current_test.spspend_resume" enable="false" priority="5" cycle="1" s_interval="300" r_interval="1"/>
		
		</loop>

		<loop name="5_Compt_Test_5-2" priority="20" cycle="1">
			<Accelerometer name="compt_test.acc" enable="true" priority="10" period="180"/>
			<CapSensor name="compt_test.cap(proximity)" enable="false" priority="10" period="180"/>
			<Compass name="compt_test.compass" enable="true" priority="10" period="180"/>
			<Gyroscope name="compt_test.gyro" enable="false" priority="10" period="180"/>
			<LightSensor name="compt_test.light" enable="false" priority="10" period="180"/>
			<LED name="compt_test.led" enable="false" priority="10" period="180"/>
			<Pressure name="compt_test.pressure" enable="true" priority="10" period="180"/>
		</loop>

		<loop name="5_Sleep_Current_Test_5-3" priority="40" cycle="1">
		
			<SuspendResume name="sleep_current_test.spspend_resume" enable="false" priority="5" cycle="1" s_interval="600" r_interval="1"/>
		
		</loop>
		
	</loop>


		
	<loop name="Step6" enable="false" priority="960" cycle="1">

		<SetChargingMode name="set_charging_mode" enable="false" priority="1" value="disable"/>
	
		<FrontCameraPic name="front_cam_pic" enable="false" priority="10" interval="5" period="300">
			<PreviewOnly>false</PreviewOnly>
			<ZoomSwitch>true</ZoomSwitch>
			<Resolution enable="true" width="640" height="480"/>
			<PicPath enable="true">/sdcard/Movies/front_cam_pic.jpg</PicPath>
		</FrontCameraPic>
		
		<MainCameraPic name="main_cam_pic" enable="false" priority="15" interval="5" period="300">
			<FlashOn>false</FlashOn>
			<PreviewOnly>false</PreviewOnly>
			<ZoomSwitch>true</ZoomSwitch>
			<Resolution enable="true" width="1024" height="768"/>
			<PicPath enable="true">/sdcard/Movies/main_cam_pic.jpg</PicPath>
		</MainCameraPic>
		
		<MainCameraPic name="main_cam_pic_flashOn" enable="false" priority="20" interval="5" period="300">
			<FlashOn>true</FlashOn>
			<PreviewOnly>false</PreviewOnly>
			<ZoomSwitch>true</ZoomSwitch>
			<Resolution enable="true" width="1024" height="768"/>
			<PicPath enable="true">/sdcard/Movies/main_cam_flash_pic.jpg</PicPath>
		</MainCameraPic>


		<FrontCameraVideo name="front_cam_video" enable="false" priority="23" period="300">
			<Resolution enable="true" width="640" height="480"/>
  			<RecordPath>/sdcard/Movies/front_cam_rec.mp4</RecordPath>
 		</FrontCameraVideo>


		<MainCameraVideo name="main_cam_video" enable="false" priority="25" period="300">
			<Resolution enable="true" width="640" height="480"/>
			<RecordPath>/sdcard/Movies/main_cam_video.mp4</RecordPath>
		</MainCameraVideo>

		<CameraSwitch name="cam_switch" enable="false" priority="30" interval="5" period="300"/>

	</loop>


	<loop name="Step7" enable="true" priority="970" cycle="1">

		<SetChargingMode name="set_charging_mode" enable="false" priority="1" value="enable"/>

		<TPSelfTest name="tp_selftest" enable="false" priority="2" interval="2" period="2">
			<TPPathWrite value="/proc/touch" data="1"/>
			<TPPathRead  value="/proc/touch" data="0"/>
		</TPSelfTest>

		<EMMC name="emmc" enable="true" priority="7" period="840"/>

		<TPSelfTest name="tp_selftest" enable="false" priority="10" interval="2" period="2">
			<TPPathWrite value="/proc/touch" data="1"/>
			<TPPathRead  value="/proc/touch" data="0"/>
		</TPSelfTest>

	</loop>


</alt_config>
